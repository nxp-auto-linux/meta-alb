+	// Physical Memory Areas
+	// =====================================================
+	lnx1_pma: lnx1_pma {
+		compatible = "phys-mem-area";
+		addr = <0x0 0x00000000>;	// Linux
+		size = <0x0 0x40000000>;	// 1GB
+	};
+
+	lnx2_pma: lnx2_pma {
+		compatible = "phys-mem-area";
+		addr = <0x0 0x40000000>;	// Linux
+		size = <0x0 0x20000000>;	// 512MB
+	};
